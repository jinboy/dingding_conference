<view class="tab-tip-container-class tab-tip-border-{{tipLayout}}">
    <view class="tab-tip-vertical-line-class" style="background:{{lineColor?lineColor:'#49a9ee' + ';'}}"></view>
    <text class="tab-tip-text" style="font-size:{{fontSize?fontSize:22}}rpx;">{{text?text:''}}</text>
    <slot name="right"></slot>
</view>
